<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title> NAM Studio </title>
  <link rel="shortcut icon" type= "image/x-icon" href="/images/nam4x.png">
  <link rel='stylesheet' href='/stylesheets/bootstrap.min.css'>
  <link rel='stylesheet' href='/stylesheets/idProduct.css'>
  
</head>
<body>
  <%- include('partials/header') %>


  <div class="container">
    <div class="row">
  
      <div id="carousel" class=" col-12 col-md-6 col-lg-6">
        <div style="background: url(/data/cws/<%=product.cw1%>); background-size: 40%"  id="cw1"></div>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" data-interval="2000" pause="hover">
          <div class="carousel-inner">
          
            <div class="carousel-item active">
     
              <img class="d-block w-100" src="/images/empty.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/mochila.png" alt="Second slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/pollera.png" alt="Third slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="/images/vestido.png" alt="Fourth slide">
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
      </div>
      <div class="detalle col-12 col-md-6 col-lg-3">
        <h2>DSN#<%=product.name%></h2>
        
        <!-- Zoom -->
        
        <div class="d-flex justify-content-center my-4 pt-3">
          <span class="font-weight-bold purple-text mr-2 mt-1"><i class="fas fa-minus" aria-hidden="true"></i></span>
          <form class="range-field w-75">
            <label for="zoom">ZOOM</label>
            <input id="zoom" class="border-1" type="range" min="0" max="100" />
          </form>
          <span class="font-weight-bold purple-text ml-2 mt-1"><i class="fas fa-plus" aria-hidden="true"></i></span>
        </div>
        
        <!-- Colorways -->
        <ul type="none">
          <li><img src="/data/cws/<%=product.cw1%>" width="40%"><a href="#"> CW 1</a></li>
          <li><img src="/data/cws/<%=product.cw2%>" width="40%"><a href="#"> CW 2</a></li>
          <li><img src="/data/cws/<%=product.cw3%>" width="40%"><a href="#"> CW 3</a></li>
        </ul>
        <div id="detalle">
          <h6>TAMAÑO REAL (cm):</h6>
          <p><%=product.size%></p>
          <h6>CATEGORÍA:</h6>
          <p><%=product.category.name%></p>
        </div>
      </div>
      
      <div class="carrito col-12 col-md-6 col-lg-3">
        <button type="button" class="btn btn-danger">Agregar al carrito</button>
      </div>
    </div>
  </div>
  
